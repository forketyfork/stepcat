{
  "name": "stepcat",
  "version": "0.1.0",
  "description": "Step-by-step agent orchestration solution",
  "main": "dist/index.js",
  "bin": {
    "stepcat": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc && npm run postbuild",
    "postbuild": "node -e \"const fs = require('fs'); const path = 'dist/cli.js'; let content = fs.readFileSync(path, 'utf8'); if (!content.startsWith('#!')) { content = '#!/usr/bin/env node\\n' + content; fs.writeFileSync(path, content); fs.chmodSync(path, '755'); }\"",
    "start": "node dist/cli.js",
    "dev": "ts-node src/cli.ts",
    "test": "jest",
    "lint": "eslint src --ext .ts"
  },
  "keywords": [
    "orchestration",
    "agent",
    "claude",
    "codex"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/claude-code": "^2.0.13",
    "@octokit/rest": "^20.1.2",
    "@openai/codex": "^0.46.0",
    "better-sqlite3": "^11.10.0",
    "commander": "^11.1.0",
    "express": "^5.1.0",
    "open": "^10.2.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.12",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.19.20",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  }
}
